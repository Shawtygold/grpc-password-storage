<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:PasswordBoxClient.MVVM.View"
                    xmlns:vm="clr-namespace:PasswordBoxClient.MVVM.ViewModel">

    <DataTemplate DataType="{x:Type TypeName=vm:PasswordsViewModel}">
        <view:PasswordsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:LoadingScreenViewModel}">
        <view:LoadingScreenView/>
    </DataTemplate>

</ResourceDictionary>